<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <context:component-scan base-package = "com"/>

    <bean id="orederService" class="com.Lesson1.OrderService"/>
    <bean id="orderDAO" class="com.Lesson1.OrderDAO"/>
    <bean id="route" class="com.Lesson1.HW1.Route"/>
    <bean id="step" class="com.Lesson1.HW1.Step"/>
    <bean id="service" class="com.Lesson1.HW1.Service"/>



</beans>